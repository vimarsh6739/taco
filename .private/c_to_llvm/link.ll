  store i32 2, i32* %b, align 4
  %0 = load i32, i32* %a, align 4
  %1 = load i32, i32* %b, align 4
  %call = call i32 (i32, i32, ...) bitcast (i32 (i32, i32)* @Sum to i32 (i32, i32, ...)*)(i32 %0, i32 %1)
