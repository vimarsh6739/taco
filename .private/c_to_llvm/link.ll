  %2 = load i32, i32* %a, align 4
  %3 = load i32, i32* %b, align 4
  %4 = load i32, i32* %c, align 4
  %call1 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([12 x i8], [12 x i8]* @.str, i64 0, i64 0), i32 %2, i32 %3, i32 %4)
  ret i32 0
}

declare dso_local i32 @printf(i8*, ...) #1

; Function Attrs: alwaysinline
define i32 @Sum(i32 %a, i32 %b) #2 {
